@model List<HotelApp.Models.AppUser>

<div class="layout-demo-wrapper">
    <div class="card shadow-lg container p-5">
        <div class="titlepage">
            <h2>Quản lý Khách hàng</h2>
        </div>
        <button class="btn btn-primary mb-4" style="width: 200px;" onclick="location.href='@Url.Action("Create","Client")'">Thêm khách hàng</button>

        <table id="ClientTableAdmin" class="table table-striped table-bordered pt-5">
            <thead class="table-dark">
                <tr>
                    <th>SST</th>
                    <th>Họ và tên</th>
                    <th>Email</th>
                    <th>Số điện thoại</th>
                    <th>Ảnh đại diện</th>
                </tr>
            </thead>
        </table>
    </div>
</div>


